{
  "name": "@4mbl/components",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./.css": "./dist/assets/components.css"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "4mbl",
  "keywords": [
    "react",
    "components"
  ],
  "scripts": {
    "ci": "pnpm run build && pnpm run format:check && pnpm run typecheck && pnpm run lint",
    "dev": "vite",
    "build": "vite build --config vite.config.ts",
    "build:app": "vite build --config vite-app.config.ts",
    "lint": "eslint src/lib",
    "format:check": "prettier --check src/lib",
    "typecheck": "tsc",
    "preview": "vite preview",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "pnpm run ci"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.19.0",
    "@tailwindcss/postcss": "^4.0.8",
    "@tailwindcss/vite": "^4.0.8",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "glob": "^11.0.1",
    "globals": "^15.14.0",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "^19",
    "react-dom": "^19",
    "tailwindcss": "^4.0.8",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "packageManager": "pnpm@10.4.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
